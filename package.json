{
  "name": "@rdf-esm/formats-common",
  "version": "0.5.5",
  "description": "Parsers and serializers for common RDF formats",
  "main": "index.js",
  "type": "module",
  "exports": {
    ".": {
      "import": "./index.js",
      "require": "./index.cjs"
    }
  },
  "files": [
    "*.js",
    "*.cjs",
    "*.d.ts"
  ],
  "scripts": {
    "lint": "standard",
    "test": "c8 --reporter=lcov mocha test/*.cjs",
    "prepack": "babel *.js --out-dir . --out-file-extension .cjs"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rdf-esm/formats-common.git"
  },
  "keywords": [
    "rdf",
    "rdfjs",
    "format",
    "parser",
    "serializer"
  ],
  "author": "Thomas Bergwinkl <bergi@axolotlfarm.org> (https://www.bergnet.org/people/bergi/card#me)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rdf-esm/formats-common/issues"
  },
  "homepage": "https://github.com/rdf-esm/formats-common",
  "dependencies": {
    "@rdf-esm/sink-map": "^0.5.5",
    "@rdfjs/parser-jsonld": "^1.2.2",
    "@rdfjs/parser-n3": "^1.1.4",
    "@rdfjs/serializer-jsonld": "^1.2.3",
    "@rdfjs/serializer-ntriples": "^1.0.3",
    "rdfxml-streaming-parser": "^1.5.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.17.0",
    "@babel/core": "^7.17.2",
    "@babel/preset-env": "^7.16.11",
    "@types/rdf-js": "^3.0.1",
    "babel-plugin-add-import-extension": "^1.6.0",
    "mocha": "^9.2.0",
    "c8": "^7.1.2",
    "standard": "^16.0.4"
  },
  "engines": {
    "node": ">=12"
  },
  "publishConfig": {
    "access": "public"
  }
}
